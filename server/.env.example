# MongoDB Connection String
MONGO_URI=mongodb://localhost:27017/social-media-business-trainer
# or for MongoDB Atlas:
# MONGO_URI=mongodb+srv://username:password@cluster.mongodb.net/social-media-business-trainer

# JWT Secret (use a strong random string in production)
JWT_SECRET=your_super_secret_jwt_key_change_this_in_production

# Google Gemini API Key
GEMINI_API_KEY=your_gemini_api_key_here

# Gemini Model (Options: gemini-1.5-pro, gemini-1.5-flash, gemini-2.0-flash-exp)
GEMINI_MODEL=gemini-1.5-pro

# Gemini Vision Model for image analysis (recommended: gemini-1.5-flash)
GEMINI_VISION_MODEL=gemini-1.5-flash

# Server Port
PORT=5000
